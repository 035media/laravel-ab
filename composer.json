{
    "name": "ben182/laravel-ab",
    "description": "",
    "keywords": [
        "ben182",
        "laravel-ab"
    ],
    "homepage": "https://github.com/ben182/laravel-ab",
    "license": "MIT License (MIT)",
    "type": "library",
    "authors": [
        {
            "name": "Ben",
            "email": "moin@benjaminbortels.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.7.* || 5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.7.*",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ben182\\AbTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ben182\\AbTesting\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ben182\\AbTesting\\AbTestingServiceProvider"
            ],
            "aliases": {
                "AbTesting": "Ben182\\AbTesting\\AbTestingFacade"
            }
        }
    }
}
